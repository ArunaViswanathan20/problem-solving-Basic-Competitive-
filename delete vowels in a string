#include <stdio.h>
#include <stdlib.h>

int main()
{
    char str[100];
    gets(str);
    for(int i=0;i<strlen(str);i++)
    {

    if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U'){
            for(int j=i;j<strlen(str);j++)
            {
            str[j]=str[j+1];
            }

    }}
    printf("%s",str);
    return 0;
}
