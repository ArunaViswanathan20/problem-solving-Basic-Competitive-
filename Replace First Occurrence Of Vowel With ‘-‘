#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    char str[100];
    printf("enter string:");
    gets(str);
    for(int i=0;i<=strlen(str);i++)
    {
        if(str[i]=='a'){
            str[i]='-';
            printf("%s",str);
            return 0;
        }
    }
    return 0;
}
