#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,n,rem,sum=0;
    printf("enter the number:");//6 28 496 8128
    scanf("%d",&n);
    for (i=1;i<n;i++)
    {
        rem=n%i;
        if(rem==0)
        {
            sum=sum+i;
        }

    }
    if (sum==n)
        printf("perfect");
    else
        printf("imperfect");
    return 0;
}
