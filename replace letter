#include <iostream>
#include<string.h>
using namespace std;

int main()
{
    cout << "Hello world!" << endl;
    char str[200],n,m;
    gets(str);
    cin>>m;
    cin>>n;
    for(int i=0;i<strlen(str);i++)
    {
        if(str[i]==m)
        {
          str[i]=n;
        }
    }
    cout<<str;
    return 0;
}
